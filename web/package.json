{
  "name": "todu-fit-web",
  "version": "1.3.1",
  "description": "A meal planning and nutrition tracking web application.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server.ts",
    "build": "tsc && vite build",
    "build:server": "esbuild server.ts --bundle --platform=node --format=esm --outfile=dist-server/server.js --packages=external",
    "start": "node dist-server/server.js",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evcraddock/todu-fit.git",
    "directory": "web"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evcraddock/todu-fit/issues"
  },
  "homepage": "https://github.com/evcraddock/todu-fit#readme",
  "dependencies": {
    "@automerge/automerge-repo": "^2.5.1",
    "@automerge/automerge-repo-network-broadcastchannel": "^2.5.1",
    "@automerge/automerge-repo-network-websocket": "^2.5.1",
    "@automerge/automerge-repo-react-hooks": "^2.5.1",
    "@automerge/automerge-repo-storage-indexeddb": "^2.5.1",
    "@automerge/react": "^2.5.1",
    "@hono/node-server": "^1.19.7",
    "@simplewebauthn/server": "^13.2.2",
    "@simplewebauthn/types": "^12.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "better-sqlite3": "^12.6.0",
    "bs58check": "^4.0.0",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.3",
    "http-proxy": "^1.18.1",
    "nodemailer": "^7.0.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.3.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0"
  }
}
